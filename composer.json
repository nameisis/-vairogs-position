{
    "name": "vairogs/vairogs-position",
    "description": "Utils: Position / Pagination library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "vairogs",
        "position",
        "pagination",
        "symfony"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "vairogs/vairogs-core": "dev-master",
        "symfony/dependency-injection": "*",
        "symfony/http-kernel": "*",
        "symfony/config": "*",
        "symfony/http-foundation": "*",
        "symfony/framework-bundle": "*",
        "doctrine/persistence": "^1",
        "doctrine/orm": "^2.6",
        "twig/twig": "^2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Utils\\Position\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vairogs\\Utils\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "4.1.*"
        }
    }
}
